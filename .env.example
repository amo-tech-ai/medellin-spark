GITHUB_TOKEN=your_github_token_here

# ───────────────────────────────
# 🤖 AI API Keys
# ───────────────────────────────
# Server-side AI keys (not exposed to browser)
ANTHROPIC_API_KEY=your_anthropic_key_here
PERPLEXITY_API_KEY=your_perplexity_key_here
OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=your_google_key_here

# ⚠️ SECURITY WARNING: Client-side API keys
# Exposing API keys to the browser is NOT recommended for production!
# Consider using a backend proxy or Edge Functions instead.
VITE_OPENAI_API_KEY=your_openai_key_here

# Ollama Cloud API Key
OLLAMA_API_KEY=your_ollama_key_here

# ===============================
# Project: medellinai (Supabase)
# Project ID / Ref
SUPABASE_PROJECT_ID=your_project_id_here
SUPABASE_PROJECT_URL=https://your-project-id.supabase.co
# ===============================

# ───────────────────────────────
# ✅ Frontend (safe to expose in Vite)
# These must be prefixed with VITE_ so your Vite app can read them.
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# (Optional) Only expose if you intentionally use it client-side.
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
VITE_SUPABASE_PROJECT_ID=your_project_id_here
# ───────────────────────────────


# ───────────────────────────────
# 🔒 Server-only (NEVER prefix with VITE, NEVER ship to browser)
# Use these in your server, Edge Functions, or CLI—NOT in the client.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here

# "Publishable/Secret" pair you listed (treat secret as server-only)
SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
SUPABASE_SECRET_KEY=your_secret_key_here

# JWT key IDs (rarely needed, but included since you provided them)
SUPABASE_JWT_CURRENT_KID=your_current_kid_here
SUPABASE_JWT_STANDBY_KID=your_standby_kid_here

# Access tokens
SUPABASE_ACCESS_TOKEN=your_access_token_here
# ───────────────────────────────
# ===================================================
# 🗄️ DATABASE CONNECTION — MedellinAI
# ===================================================

# Correctly formatted password with special character #
POSTGRES_PASSWORD="YourSecurePassword#Here"

# Direct connection (admin, migrations)
DATABASE_URL="postgresql://postgres:YourEncodedPassword@db.your-project-id.supabase.co:5432/postgres"

# Transaction pooler (for serverless)
POOLER_URL="postgresql://postgres.your-project-id:YourEncodedPassword@aws-1-us-east-2.pooler.supabase.com:6543/postgres"
